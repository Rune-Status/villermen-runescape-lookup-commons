{
    "name": "villermen/runescape-lookup-commons",
    "description": "A PHP library containing RuneScape lookup components.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-simplexml": "*"
    },
    "autoload": {
        "psr-4": {
            "Villermen\\RuneScape\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "symfony/var-dumper": "^5.4"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0"
        }
    }
}
